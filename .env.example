# Database Configuration
DATABASE_URL=jdbc:postgresql://your-rds-endpoint:5432/dealharbor
DATABASE_USERNAME=postgres
DATABASE_PASSWORD=your-secure-password

# Redis Configuration (AWS ElastiCache)
REDIS_HOST=your-elasticache-endpoint
REDIS_PORT=6379
REDIS_PASSWORD=

# JWT Configuration
JWT_SECRET=your-super-secure-jwt-secret-key-at-least-256-bits
JWT_ACCESS_TOKEN_VALIDITY=900000

# Mail Configuration
MAIL_HOST=smtp.gmail.com
MAIL_PORT=587
MAIL_USERNAME=your-email@gmail.com
MAIL_PASSWORD=your-app-password
MAIL_FROM=your-email@gmail.com

# OAuth2 Configuration
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
GITHUB_CLIENT_ID=your-github-client-id
GITHUB_CLIENT_SECRET=your-github-client-secret
OAUTH2_REDIRECT_URI=https://your-frontend-domain.com/oauth2/redirect

# Supabase Storage Configuration
SUPABASE_PROJECT_URL=https://your-project.supabase.co
SUPABASE_BUCKET_NAME=product-images
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key
SUPABASE_PUBLIC_URL=https://your-project.supabase.co/storage/v1/object/public/product-images

# AWS Specific
AWS_REGION=ap-south-1
PORT=8080
